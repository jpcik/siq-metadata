<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<managed-bean>
		<managed-bean-name>queryManager</managed-bean-name>
		<managed-bean-class>es.upm.fi.dia.oeg.integrator.web.QueryManager</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>bboxLat1</property-name>
			<property-class>java.lang.String</property-class>
			<value>43.5</value>
		</managed-property>
		<managed-property>
			<property-name>bboxLon1</property-name>
			<property-class>java.lang.String</property-class>
			<value>1.73</value>
		</managed-property>
		<managed-property>
			<property-name>bboxLat2</property-name>
			<property-class>java.lang.String</property-class>
			<value>48.5</value>
		</managed-property>
		<managed-property>
			<property-name>bboxLon2</property-name>
			<property-class>java.lang.String</property-class>
			<value>17.5</value>
		</managed-property>
		<managed-property>
			<property-name>centerLat</property-name>
			<property-class>java.lang.String</property-class>
			<value>46</value>
		</managed-property>
		<managed-property>
			<property-name>centerLon</property-name>
			<property-class>java.lang.String</property-class>
			<value>9.7</value>
		</managed-property>
		<managed-property>
			<property-name>zoom</property-name>
			<property-class>java.lang.String</property-class>
			<value>6</value>
		</managed-property>
	</managed-bean>
	<navigation-rule>
		<display-name>index.xhtml</display-name>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>search.xhtml</display-name>
		<from-view-id>/search.xhtml</from-view-id>
		<navigation-case>
			<to-view-id>/search.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
